{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
  	{
  	  "name": "client",
  	  "image": "mowen98/multi-client",
  	  "hostname": "client",
  	  "essential": false
  	},
  	{
  	  "name": "server",
  	  "image": "mowen98/multi-server",
  	  "hostname": "api",
  	  "essential": false
  	},
  	{
  	  "name": "worker",
  	  "image": "mowen98/multi-worker",
  	  "hostname": "worker",
  	  "essential": false
  	},
  	{
  	  "name": "nginx",
  	  "image": "mowen98/multi-nginx",
  	  "hostname": "nginx",
  	  "essential": true,
  	  "portMappings": [
  	  	{
  	  	  "hostPort": 80,
  	  	  "containerPort": 80
  	  	}
  	  ],
  	  "links": ["client", "server"]
  	}
  ]
}